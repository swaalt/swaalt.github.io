.contact-me-icons {
  flex-direction: row;
  justify-content: center;
  align-items: flex-start;
  margin: 0 30px 0 0;
  padding: 0;
  list-style: none;
  display: flex;
  position: relative;
  bottom: 0;
}

.contact-me-icons ul {
  flex-wrap: nowrap;
  padding-left: 0;
  display: flex;
}

.contact-me-icons ul li a {
  text-decoration: none;
}

.contact-me-icons ul li i {
  color: #3d3d3f;
  font-size: 30px;
  text-decoration: none;
  transition: all .5s;
}

.contact-me-icons ul li i:hover {
  color: #207de9;
}

.contact-me-icons ul li {
  justify-content: flex-start;
  align-items: center;
  padding: 5px;
  display: flex;
}

.contact-section h2 {
  color: #0b1720;
  font-size: 55px;
  font-weight: bold;
}

.contact-section {
  padding: var(--section-distance);
  flex-direction: column;
  justify-content: center;
  align-items: center;
  display: flex;
}

.kiwi {
  width: 35px;
  height: 35px;
  margin-left: 20px;
}

